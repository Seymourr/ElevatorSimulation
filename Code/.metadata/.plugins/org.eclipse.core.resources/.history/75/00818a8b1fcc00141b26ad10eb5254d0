import java.util.ArrayList;


public class SingleAutomatic extends Algorithm{
	
	public SingleAutomatic(ElevatorSpecs spec)
	{
		this.spec = spec;
	}
	
	public ArrayList<Elevator> manageCalls(ArrayList<Call> traffic, ArrayList<Elevator>localElevators, ArrayList<Elevator>shuttleElevators, ArrayList<Passenger>newCalls)
	{
		ArrayList<Elevator> allElev = localElevators;
		allElev.addAll(shuttleElevators);
	}
}
